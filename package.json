{
  "name": "fyty-api-node",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "lint:scripts": "eslint './src/**/*.{js,ts}'",
    "fix:scripts": "eslint './src/**/*.{js,ts}' --fix",
    "prettier": "prettier --write './src/**/*.{js,ts,jsx,tsx}'",
    "format": "npm run prettier && npm run fix:scripts && fix:styles",
    "check": "tsc --noEmit",
    "prisma:migrate": "prisma db push --schema=./src/prisma/schema.prisma",
    "prisma:gen": "prisma generate --schema=./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.9",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/websockets": "^9.3.9",
    "@prisma/client": "^4.11.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.1.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "firebase": "^9.17.2",
    "non-null": "^2.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.9.0",
    "prisma": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.3.0",
    "rxjs": "^7.8.0",
    "urijs": "^1.19.11",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.2.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.9",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.6",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@types/urijs": "^1.19.19",
    "jest": "29.4.3",
    "prisma-dbml-generator": "^0.10.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "3.14.2",
    "typescript": "4.9.5"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
