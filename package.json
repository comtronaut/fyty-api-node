{
  "name": "fyty-api-node",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "build": "nest build",
    "build:docs": "nest build && node dist/build-openapi.js",
    "start": "nest start",
    "dev": "nest start --watch",
    "lint:scripts": "eslint './src/**/*.{js,ts}'",
    "fix:scripts": "eslint './src/**/*.{js,ts}' --fix",
    "prettier": "prettier --write './src/**/*.{js,ts,jsx,tsx}'",
    "format": "npm run prettier && npm run fix:scripts",
    "check": "tsc --noEmit",
    "prisma:migrate": "prisma db push --schema=./src/prisma/schema.prisma",
    "prisma:gen": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:seed": "ts-node src/prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@nestjs/event-emitter": "^2.0.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-socket.io": "^10.1.3",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/swagger": "^7.1.7",
    "@nestjs/websockets": "^10.1.3",
    "@prisma/client": "^5.1.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.3",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "nestjs-zod": "^2.3.3",
    "non-null": "^2.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.2",
    "prisma": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-pattern": "^5.0.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.4.8",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.2",
    "prisma-dbml-generator": "^0.10.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsdef": "^0.0.14",
    "typescript": "^5.1.6",
    "zod-prisma-types": "^2.7.9"
  },
  "optionalDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-yml": "^1.8.0",
    "prettier": "^3.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
